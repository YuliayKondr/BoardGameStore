@model IEnumerable<string>

@Html.ActionLink("Домой", "List", "BoardGame", null,
    new { @class = "btn btn-primary" })
@foreach (var link in Model)
{
    @Html.RouteLink(link, new
{
    controller = "BoardGame",
    action = "List",
    category = link,
    page = 1
}, new
{
    @class = "btn btn-primary" + (link == ViewBag.Cat ? "btn-default" : "")
    })
   
}
